<UserControl x:Class="ImagesApp.ImagePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImagesApp"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="300">
    <Grid Margin="0,0,0,10">
        <Image Name="firstImage" HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="128"/>
        <Image Name="secondImage" HorizontalAlignment="Left" Height="100" Margin="162,10,0,0" VerticalAlignment="Top" Width="128"/>
        <Image Name="thirdImage" HorizontalAlignment="Left" Height="100" Margin="10,128,0,0" VerticalAlignment="Top" Width="128"/>
        <Image Name="forthImage" HorizontalAlignment="Left" Height="100" Margin="162,128,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
</UserControl>
